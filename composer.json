{
    "name": "dnabeast/tusk",
    "description": "Creates a plugin file to replace text in a string with any php or html code",
    "license": "MIT",
    "keywords": ["plugin", "blade"],
    "authors": [
        {
            "name": "Dan Beeston",
            "email": "dan@civicnet.com.au"
        }
    ],
    "require": {
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "DNABeast\\Tusk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DNABeast\\Tusk\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "DNABeast\\Tusk\\TuskServiceProvider"
            ]
        }
    }

}
